        <arguments>
          <argument>
            <kind>1</kind>
            <string>
//Change animations and object properties based on state
switch (state){

  case "idle":
    sprite_index = spr_bluemanidle;
  break;

  case "notice_left":
    image_speed = 0.5;
    sprite_index = spr_blueman_n_l;
  break;
  
  //Picking up gem
  case "pickup":
    image_speed = 0.5;
    sprite_index = spr_blueman_pick;
  break;
  
  case "notice_right":
    image_speed = 0.5;
    sprite_index = spr_blueman_n_r;
  break;
  
  case "moving_left":
    image_speed = 0.5;
    sprite_index = spr_bluemanw_l;
    //Screen shake effect
    view_xview = random_range(-2, 2)
    view_yview = random_range(-2, 2)
    if x &gt; nearest_gem.x { x-= 10 }
    
  break;
  
  case "moving_right":
    image_speed = 0.5;
    sprite_index = spr_bluemanw_r;
    //Screen shake effect
    view_xview = random_range(-2, 2)
    view_yview = random_range(-2, 2)
    if x &lt; nearest_gem.x { x+= 10 }
  break;
  
  //Called to house
  case "called":
    if x &lt; room_width-200 { x+= 10 }
    view_xview = random_range(-2, 2)
    view_yview = random_range(-2, 2)
    sprite_index = spr_bluemanw_r;
  break;
}

  
   
   
</string>
