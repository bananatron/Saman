  }

  if ddirection == "right" {
    sprite_running = spr_rook1running_r
    sprite_running_loaded = spr_rook1runloaded_r
  }

/**
//If not at destination, they're running
if global.destx != self.x &amp;&amp; loaded = false {
  state = "running"
}
//If not at destination &amp; loaded, then running loaded
if global.destx != self.x &amp;&amp; loaded = true {
  state = "running_loaded"
}
//If at destination and not picking up, set to idle
if global.destx == self.x &amp;&amp; picking_up = false {
  state = "idle"
}
**/

//Change animations and object properties based on state
switch (state){

  case "idle":
    movespeed = 5;
    image_speed = 0.8;
    
    if loaded == false {
     sprite_index = spr_rook1idle;
    }
    else {
      sprite_index = spr_rook1idleloaded;
    }

  break; 
