  break;
  
  case "unburrowing":
    image_speed = 1;
    sprite_index = spr_rookBurrow_u;
  break;
  
  case "breaking":
    image_speed = 0.8;
    sprite_index = spr_rookBreaking;
  break;
  
  case "broken":
    image_speed = 0;
    sprite_index = spr_rookBreaking;
    image_index = 13;
  break;
  
}


   
