    scr_rookBreaking();
 }
 if place_meeting(x, y, obj_masterBludger) &amp;&amp; state != "burrowed" {
    scr_rookBreaking();
 }
 //Lizard birds can break you even when burrowed.
 if place_meeting(x, y, obj_liz) {
     scr_rookBreaking();
 }
}

//Determine direction of movement and set sprite

  if ddirection == "left"{
    sprite_running = spr_rook1running_l
    sprite_running_loaded = spr_rook1runloaded_l
  }

  if ddirection == "right" {
    sprite_running = spr_rook1running_r
    sprite_running_loaded = spr_rook1runloaded_r
  }

//Change animations and ~object properties based on state
switch (state){

  case "idle":
    movespeed = 5;
    image_speed = 0.8;
    
    if loaded == false {
     sprite_index = spr_rook1idle;
    }
    else {
      sprite_index = spr_rook1idleloaded;
    }

  break; 
  case "running":
    image_speed = 1;

    sprite_index = sprite_running;
  break;
  
  case "running_loaded":
